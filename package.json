{
  "name": "pibspa",
  "version": "0.1.5",
  "private": true,
  "license": "UNLICENSED",
  "licenseText": "SEE LICENSE IN LICENSE",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:spa": "node scripts/build-spa.js",
    "build:spa:prepare": "echo 'Use build:spa instead'",
    "build:spa:cleanup": "node scripts/cleanup-spa.js",
    "build:ssr": "react-router build --config react-router.ssr.config.ts",
    "dev": "react-router dev",
    "dev:spa": "vite --config vite.spa.config.ts",
    "dev:ssr": "react-router dev --config react-router.ssr.config.ts",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio --port 5556",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "ios:build": "npm run build:spa && npx tauri ios build --ci",
    "ios:build:sim": "npm run build:spa && npx tauri ios build --target aarch64-sim --ci",
    "ios:run": "node scripts/ios-run.js",
    "ios:archive": "node scripts/archive-ios.js",
    "ios:dev": "echo '12' | npx tauri ios dev",
    "ios:beta": "fastlane ios beta",
    "ios:register": "fastlane ios register_device",
    "android:build": "npm run build:spa && npx tauri android build",
    "android:run": "node scripts/android-run.js",
    "android:dev": "npx tauri android dev",
    "android:internal": "fastlane android internal",
    "android:alpha": "fastlane android alpha",
    "android:beta": "fastlane android beta_auto",
    "android:sign": "fastlane android setup_signing",
    "android:bundle": "cd ./src-tauri/gen/android && ./gradlew bundleRelease",
    "check-versions": "./scripts/check-versions.sh",
    "bump": "./scripts/bump.sh",
    "sync": "./scripts/bump.sh --sync",
    "update-version-status": "./scripts/update-version-status.sh",
    "update-version-status-auto": "./scripts/update-version-status-auto.sh",
    "generate-changelog": "./scripts/generate-changelog.sh",
    "tauri": "npx tauri",
    "tauri:dev": "npx tauri dev",
    "tauri:build": "npm run build:spa && npx tauri build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-router/fs-routes": "^7.3.0",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "class-variance-authority": "^0.7.1",
    "classix": "^2.2.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "isbot": "^5.1.17",
    "lucide-react": "^0.483.0",
    "prisma": "^6.15.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.4.3",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "recharts": "^3.1.0",
    "rxdb": "^16.8.1",
    "rxjs": "^7.8.2",
    "short-unique-id": "^5.2.0",
    "tailwind-merge": "^3.0.2",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.2.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.3.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tauri-apps/cli": "^2.3.1",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
