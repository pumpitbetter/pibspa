{
  "name": "pibspa",
  "version": "0.1.4",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:spa",
    "build:spa": "./scripts/setup-spa-routes.sh && react-router build && ./scripts/cleanup-spa-routes.sh",
    "build:ssr": "./scripts/setup-ssr-routes.sh && react-router build && ./scripts/cleanup-ssr-routes.sh",
    "build:all": "npm run build:spa && npm run build:ssr",
    "build:deploy": "./scripts/create-combined-server.sh && npm run build:all",
    "dev": "npm run dev:spa",
        "dev:spa": "./scripts/dev-spa-isolated.sh",
    "dev:ssr": "./scripts/dev-ssr-isolated.sh",
    "dev:ssr:express": "npm run build:ssr && npm run start:combined",
    "dev:combined": "npm run build:all && npm run start:combined",
    "postinstall": "./scripts/install-dev-dependencies.sh",
    "cleanup-spa-watcher": "./scripts/cleanup-spa-watcher.sh",
    "cleanup-dev-servers": "./scripts/cleanup-dev-servers.sh",
    "link-spa-routes": "./scripts/link-spa-routes.sh",
    "link-ssr-routes": "./scripts/link-ssr-routes.sh",
    "start": "react-router-serve ./build/server/index.js",
    "start:ssr": "node ./build/ssr/server/index.js",
    "start:combined": "node server.js",
    "dev:ssr:serve": "npm run build:ssr && npm run start:combined",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "ios:build": "npm run build:spa && npx tauri ios build",
    "ios:dev": "echo '12' | npx tauri ios dev",
    "ios:beta": "fastlane ios beta",
    "ios:register": "fastlane ios register_device",
    "ios:archive": "fastlane ios build",
    "android:build": "npm run build:spa && npx tauri android build",
    "android:dev": "npx tauri android dev",
    "android:internal": "fastlane android internal",
    "android:alpha": "fastlane android alpha",
    "android:beta": "fastlane android beta_auto",
    "android:sign": "fastlane android setup_signing",
    "android:bundle": "cd ./src-tauri/gen/android && ./gradlew bundleRelease",
    "check-versions": "./scripts/check-versions.sh",
    "bump": "./scripts/bump.sh",
    "sync": "./scripts/bump.sh --sync",
    "update-version-status": "./scripts/update-version-status.sh",
    "update-version-status-auto": "./scripts/update-version-status-auto.sh",
    "generate-changelog": "./scripts/generate-changelog.sh",
    "tauri": "npx tauri",
    "tauri:dev": "npx tauri dev",
    "tauri:build": "npm run build && npx tauri build",
    "deploy:test": "./scripts/deploy-test.sh",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "fly:login": "flyctl auth login",
    "fly:status": "flyctl status",
    "fly:logs": "flyctl logs",
    "fly:ssh": "flyctl ssh console"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-router/fs-routes": "^7.3.0",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "class-variance-authority": "^0.7.1",
    "classix": "^2.2.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "express": "^5.1.0",
    "isbot": "^5.1.17",
    "lucide-react": "^0.483.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.4.3",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "recharts": "^3.1.0",
    "rxdb": "^16.8.1",
    "rxjs": "^7.8.2",
    "short-unique-id": "^5.2.0",
    "tailwind-merge": "^3.0.2",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.2.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.3.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tauri-apps/cli": "^2.3.1",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "http-proxy-middleware": "^3.0.5",
    "prettier": "^3.6.2",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
