{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Pump It Better",
  "version": "0.1.5",
  "identifier": "com.pumpitbetter.app",
  "build": {
    "frontendDist": "../build/spa/client",
    "devUrl": "http://localhost:5175/",
    "beforeDevCommand": "npm run dev:spa",
    "beforeBuildCommand": "npm run build:spa"
  },
  "app": {
    "windows": [
      {
        "title": "Pump It Better",
        "width": 393,
        "height": 852,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "37F72L478M"
    }
  }
}
